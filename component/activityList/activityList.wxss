.wrapper {
  width: 100%;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  background: #ffffff;
}

.wrapper-image {
  width: 100%;
  height: 326rpx;
  overflow: hidden;
}

.wrapper-box {
  box-sizing: border-box;
  background: #fff;
  padding: 20rpx 30rpx;
}

.wrapper-name {
  color: #000;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.wrapper-info {
  font-size: 28rpx;
  color: #888;
}
